<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello gw networking sdk!"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/get_engr_mode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get engr mode"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="77dp" />

    <Button
        android:id="@+id/reset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="reset"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="200dp" />

    <Button
        android:id="@+id/dp_send"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="send DP"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="300dp" />

    <Button
        android:id="@+id/sync_net_mode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sync net mode"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="400dp" />

    <Button
        android:id="@+id/set_secruity_mode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="set security mode"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="500dp" />

    <Button
        android:id="@+id/get_secruity_mode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get security mode"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="600dp" />

    <Button
        android:id="@+id/sync_alarm_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="sync alarm status"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="700dp" />

    <Button
        android:id="@+id/sub_device_heartbeat_man"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="start sub-device heartbeat"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="700dp" />

    <Button
        android:id="@+id/update_subdevice_onoffline_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="update sub-device on/offline status"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="700dp" />

    <Button
        android:id="@+id/traversal_device"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="traversal device"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="700dp" />

    <Button
        android:id="@+id/droorlock_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="门锁测试"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="900dp" />

</LinearLayout>

